{
  "name": "redux-universal-boilerplate",
  "description": "Boilerplate of an isomorphic (universal) react application",
  "author": "Sergey Ivanov <xufocoder@gmail.com>",
  "license": "MIT",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ufocoder/redux-universal-boilerplate"
  },
  "homepage": "https://github.com/ufocoder/redux-universal-boilerplate",
  "keywords": [
    "react",
    "react-router",
    "react-redux",
    "redux",
    "universal",
    "isomorphic",
    "webpack",
    "express",
    "starter-kit",
    "boilerplate",
    "babel"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "forever --minUptime 1000 --spinSleepTime 1000 -c \"node --harmony\" ./bin/server.js",
    "clean": "node bin/clean.js",
    "build:server": "webpack --config webpack/server/config.js",
    "build:client": "webpack --config webpack/client/config.js",
    "build": "npm-run-all clean --parallel build:*",
    "watch": "npm-run-all clean --parallel watch:* wait:server",
    "wait:server": "just-wait --pattern \"dist/*.js\" && npm run start",
    "watch:server": "webpack --config webpack/server/watch.js",
    "watch:client": "webpack-dev-server --config webpack/client/watch.js",
    "test": "karma start ./tests/karma.config.js --single-run",
    "test:watch": "karma start ./tests/karma.config.js no-single-run",
    "lint": "eslint ./src ./bin ./webpack ./tests -c ./.eslintrc"
  },
  "dependencies": {
    "axios": "^0.15.0",
    "babel": "^6.5.2",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "body-parser": "^1.15.2",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "forever": "^0.15.1",
    "json-loader": "^0.5.4",
    "locale": "^0.1.0",
    "lodash": "^4.16.4",
    "node-noop": "^1.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-helmet": "^3.1.0",
    "react-redux": "^4.4.0",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.6",
    "react-tap-event-plugin": "^1.0.0",
    "redial": "^0.5.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.0.1",
    "semantic-ui-css": "^2.2.4",
    "serialize-javascript": "^1.3.0",
    "style-loader": "^0.13.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-isomorphic-tools": "^2.6.3"
  },
  "devDependencies": {
    "babel-eslint": "^7.0.0",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "enzyme": "^2.4.1",
    "eslint": "^3.6.0",
    "eslint-config-google": "^0.6.0",
    "eslint-plugin-react": "^6.3.0",
    "just-wait": "^1.0.5",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "npm-run-all": "^3.1.0",
    "phantomjs-prebuilt": "^2.1.12",
    "react-addons-test-utils": "^15.3.2",
    "react-hot-loader": "^1.3.0",
    "sinon": "^1.17.5",
    "webpack-dev-server": "^1.15.2",
    "webpack-node-externals": "^1.4.3"
  },
  "engines": {
    "node": ">=4.0.0"
  }
}
